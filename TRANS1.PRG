SET TALK OFF
SET ECHO OFF
SET DELE ON
SELECT 1
USE APPERSON INDEX APPERSO1
SELECT 2
USE APCONPER INDEX APCONPER
SELECT APPERSON
GO TOP
DO WHILE .NOT. EOF()
   SELECT APCONPER
   SEEK APPERSON.CEDULA+"2005"
   IF .NOT.FOUND()
      @ 24,0
      @ 24,0 SAY APPERSON.CEDULA+" SIN CODIGO DE CAJA"
      READ
   ELSE
      REPLACE APPERSON.CTACAJA WITH APCONPER.REFER
   ENDIF
   SELECT APPERSON 
   SKIP
ENDDO
CLOSE ALL
RETURN